{
  "database": {
    "redis": {
      "cluster": false,
      "options": [{
        "host": "localhost",
        "port": 6379
      }]
    },
    "mysql": {
      "username": "username",
      "password": "password",
      "database": "database",
      "host": "localhost",
      "port": 3306,
      "dialect": "mysql",
      "operatorsAliases": false,
      "logging": false,
      "timezone": "+08:00",
      "define": {
        "freezeTableName": true,
        "timestamp": true,
        "charset": "utf8"
      }
    }
  },
  "mocha": {
    "timeout": 5000
  },
  "log": {
    "level": "debug",
    "colorize": true
  },
  "mqClient": {
    "consumerGroup": {
      "options": {
        "kafkaHost": "127.0.0.1:9092",
        "groupId": "lora-network-connector-consumer",
        "sessionTimeout": 15000,
        "protocol": ["roundrobin"],
        "fromOffset": "latest"
      },
      "topics": [
        "NC-sub"
      ]
    },
    "client": {
      "kafkaHost": "127.0.0.1:9092",
      "clientId": "lora-network-connector-producer"
    },
    "producer": {
      "requireAcks": 1,
      "ackTimeoutMs": 100,
      "partitionerType": 2
    },
    "topics": {
      "pubToServer": "NS-sub"
    }
  },
  "udp": {
    "port": 12234
  }
}
